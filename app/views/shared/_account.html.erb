<section class="account_information">
	<% if customer_signed_in? %>
		<em>Signed in as <%= current_customer.email %></em>
		<nav><%= link_to "Dashboard", customer_dashboard_path %> | <%= link_to "Sign Out", 
			destroy_user_session_path %></nav>
	<% else %>
		<%= link_to "Sign In", new_customer_session_path %> | <%= link_to "Create an Account", 
			new_customer_registration_path %>
	<% end %>
</section>
